{% schema %}
{
"name": "Header",
"settings": [
{
"type": "image_picker",
"id": "logo",
"label": "Site Logo"
},
{
"type": "link_list",
"id": "header_menu",
"label": "Header Menu",
"default": "main-menu"
},
{
"type": "checkbox",
"id": "header_search_enable",
"label": "Header Area Search Show",
"default": true
},
{
"type": "checkbox",
"id": "header_cart_show",
"label": "Header Cart show",
"default": true
},
{
"type": "checkbox",
"id": "header_whislist",
"label": "Header Wishlist show",
"default": true
},
{
"type": "checkbox",
"id": "header_account",
"label": "Header Account Show",
"default": true
}
]
}
{% endschema %}