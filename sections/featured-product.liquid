                    <a class="position-relative d-block" href="{{product.url}}">
                      <img
                        class="width100 height100"
                        src="{{product.featured_image | image_url }}"
                        alt="product">
                    </a>
                    <ul class="product-action d-flex position-absolute transition-3">
                      <li
                        data-toggle="tooltip"
                        data-placement="top"
                        title="Add to Cart">
                        <a href="cart.html" class="text-center mb-10 white-bg primary-color d-block">
                          <span>
                            <i class="far fa-shopping-cart"></i>
                          </span>
                        </a>
                      </li>
                      <li
                        data-toggle="tooltip"
                        data-placement="top"
                        title="Add to Wishlist">
